{
    "name": "zae/dom",
    "description": "PHP DOM",
    "keywords": [
        "php", "dom"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "illuminate/support": "^5.1 || ^6.0",
        "symfony/css-selector": "^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Zae\\DOM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zae\\DOM\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -vvv --testdox"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
