{
    "name": "zae/dom",
    "description": "PHP DOM",
    "keywords": [
        "php", "dom"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "illuminate/support": "^5.1 || ^6.0",
        "symfony/css-selector": "^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "infection/infection": "^0.15.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "vimeo/psalm": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Zae\\DOM\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zae\\DOM\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -vvv --testdox",
        "psalm": "vendor/bin/psalm",
        "test-coverage": "@php -dpcov.enabled=1 -dpcov.directory=src -dpcov.exclude=~vendor~ vendor/bin/phpunit --coverage-text --coverage-clover=clover.xml",
        "infection": "vendor/bin/infection -j4 --only-covered --min-covered-msi=100"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
